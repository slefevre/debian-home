	name = Steve Lefevre
	email = lefevre.10@osu.edu
[push]
	default = simple
[core]
    fileMode = false
[alias]
    introduced = log --diff-filter=A --

    branch-excise = ! "gitddd () { git branch -D $1; git push origin --delete $1; }; gitddd"

    overview = ! "git remote -v; git branch; git status;  git show --name-status;"

    purify = ! "giteee() { git reset --hard; git clean -d -f; }; giteee"

    unstage = reset
